{
  "name": "commander-js-demo",
  "version": "1.0.0",
  "description": "Comprehensive Commander.js demo covering all aspects with beautiful UI",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx --watch src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    
    "cli": "tsx src/index.ts",
    "help": "tsx src/index.ts --help",
    "examples": "tsx src/index.ts examples"
  },
  "bin": {
    "demo-cli": "./index.js"
  },
  "keywords": [
    "commander",
    "cli",
    "demo",
    "node"
  ],
  "author": "Milan Dadhaniya",
  "license": "MIT",
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "figlet": "^1.9.1",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.9.4",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
